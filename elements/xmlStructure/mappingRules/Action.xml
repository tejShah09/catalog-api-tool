	<Definition>
		<ClassType>{{action.getProperites.actionname}}</ClassType>
		<Intent>{{parent.{{intent}}}}</Intent>
		<ItemElements>
			<ItemElement>
				<Intent>{{parent.{{intent}}}}</Intent>
				<Name>Name</Name>
				<Type>Any</Type>
				<Values>
					<ValueDefinition>
						<Value>{{name}}</Value>
