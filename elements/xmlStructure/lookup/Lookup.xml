<?xml version="1.0" encoding="UTF-8"?>
<CatalogItem xmlns="http://schema.sigma-systems.com/catalog.api/models">
  <ClassType>{{lookupclassname}}</ClassType>
  <Intent>{{mainAction.{{intent}}}}</Intent>
  <ItemElements>
    <ItemElement>
      <Intent>{{intent}}</Intent>
      <Name>Name</Name>
      <Type>Any</Type>
      <Values>
        <ValueDefinition>
          <Value>{{name}}</Value>
        </ValueDefinition>
      </Values>
    </ItemElement>
  </ItemElements>
  <PublicID>{{mainGuid.{{action.getFactGuidbyName.valueid.name.lookupclassname}}}}</PublicID>
</CatalogItem>